{
    "name": "@voltz-protocol/core-utils",
    "version": "1.0.0",
    "publishConfig":{
      "access": "restricted",
      "registry":"https://registry.npmjs.org/"
    }, 
    "description": "Common JS utils for Voltz",
    "files": [
      "utils/**/*.js",
      "utils/**/*.js.map",
      "utils/**/*.d.ts",
      "utils/**/*.d.ts.map"
    ],
    "scripts": {
      "build": "tsc --project src/tsconfig.json",
      "watch": "npm run build -- --watch",
      "test": "mocha --require ts-node/register",
